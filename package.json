{
  "name": "can-i-use-npm",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cocos.github.io/cocos-can-i-use-npm/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "compass-mixins": "^0.12.10",
    "react": "^17.0.2",
    "react-async": "^10.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "build": "env-cmd -f .env.dev react-scripts build",
    "build:deploy": "env-cmd -f .env.deploy react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build:deploy",
    "deploy": "gh-pages -d build -b dist",
    "build:database-schema": "typescript-json-schema --required --strictNullChecks ./src/Database.ts CanIUseNpmDatabase --out public/database/schema.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.2.3",
    "typescript-json-schema": "^0.50.1"
  }
}
